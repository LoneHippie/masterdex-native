{
   "extends": "expo/tsconfig.base",
   "compilerOptions": {
      "strict": true,
      "module": "esnext",
      "baseUrl": ".",
      "paths": {
         "~app_tools/*": ["./src/tools/*"],
         "~app_contexts/*": ["./src/contexts/*"],
         "~app_interfaces/*": ["./src/interfaces/*"],
         "~app_sections/*": ["src/sections/*"]
      }
   },
   "include": ["./src/**/*", "./App.tsx", "./src/**/*", "src/**/*", "src/**/*.*"],
   "exclude": ["node_modules", "babel.config.js"]
}
